Proceso Convertidor
	
	Definir km,cm,m Como Real;
	
	Escribir "Escribe los kilometros que deseas convertir a centimetros y metros";
	Leer km;
	
	cm <- km * 100000;
	m <- km * 1000;
	Escribir "Entonces ",km, " km son:";
	Escribir "";
	Escribir cm, " centimetros";
	Escribir "";
	Escribir m, " metros";
	
FinProceso